alias: Predbat Total V1.0
description: ""
triggers:
  - entity_id:
      - binary_sensor.predbat_exporting
    id: Predbat_export_on
    trigger: state
    to: "on"
    for:
      hours: 0
      minutes: 0
      seconds: 3
  - entity_id:
      - binary_sensor.predbat_charging
    id: Predbat_charging_on
    trigger: state
    to: "on"
    for:
      hours: 0
      minutes: 0
      seconds: 3
  - trigger: state
    entity_id:
      - predbat.best_charge_limit
    id: charge limit changes
    for:
      hours: 0
      minutes: 0
      seconds: 3
  - entity_id:
      - predbat.status
    id: Status changes
    trigger: state
    for:
      hours: 0
      minutes: 0
      seconds: 3
  - trigger: state
    entity_id:
      - binary_sensor.predbat_car_charging_slot
    for:
      hours: 0
      minutes: 0
      seconds: 3
    id: Predbat car charging slot
conditions:
  - condition: state
    entity_id: switch.predbat_set_read_only
    state: "off"
actions:
  - alias: Charge, Discharge
    choose:
      - conditions:
          - condition: and
            conditions:
              - condition: state
                entity_id: predbat.status
                state: Charging
              - condition: state
                entity_id: binary_sensor.predbat_charging
                state: "on"
        sequence:
          - alias: 06:00-23:30 Charge
            if:
              - condition: and
                conditions:
                  - condition: time
                    after: "06:00:00"
                    before: "23:30:00"
                  - condition: numeric_state
                    entity_id: sensor.battery_state_of_capacity
                    above: 0
                    below: 97
                    enabled: true
            then:
              - if:
                  - condition: state
                    entity_id: binary_sensor.predbat_car_charging_slot
                    state: "on"
                then:
                  - action: huawei_solar.forcible_charge_soc
                    data:
                      target_soc: >-
                        {% set value = states('predbat.best_charge_limit') |
                        int(0) %} {{ value if value >= 12 else 12 }}
                      power: 5000
                      device_id: 6893ee838a0972bd3e50d9e0faa0ba5a
                  - delay:
                      hours: 0
                      minutes: 0
                      seconds: 3
                      milliseconds: 0
                  - action: script.emhass_charging_power_2500
                    data: {}
                  - delay:
                      hours: 0
                      minutes: 0
                      seconds: 3
                      milliseconds: 0
                  - action: script.huawei_discharging_power_0w
                    data: {}
                else:
                  - if:
                      - condition: state
                        entity_id: predbat.status
                        state: Charging
                    then:
                      - action: huawei_solar.forcible_charge_soc
                        data:
                          target_soc: >-
                            {% set value = states('predbat.best_charge_limit') |
                            int(0) %} {{ value if value >= 12 else 12 }}
                          power: 5000
                          device_id: 6893ee838a0972bd3e50d9e0faa0ba5a
                      - delay:
                          hours: 0
                          minutes: 0
                          seconds: 3
                          milliseconds: 0
                      - action: script.huawei_charging_power_4500w
                        metadata: {}
                        data: {}
                      - delay:
                          hours: 0
                          minutes: 0
                          seconds: 3
                          milliseconds: 0
                      - action: script.huawei_discharging_power_0w
                        metadata: {}
                        data: {}
                    enabled: true
            enabled: true
          - alias: 23:30-06:00 Charge
            if:
              - condition: time
                after: "23:30:00"
                before: "06:00:00"
              - condition: and
                conditions:
                  - condition: numeric_state
                    entity_id: sensor.battery_state_of_capacity
                    above: 0
                    below: 96
            then:
              - if:
                  - condition: state
                    entity_id: binary_sensor.predbat_car_charging_slot
                    state: "on"
                then:
                  - action: huawei_solar.forcible_charge_soc
                    data:
                      target_soc: >-
                        {% set value = states('predbat.best_charge_limit') |
                        int(0) %} {{ value if value >= 12 else 12 }}
                      power: 5000
                      device_id: 6893ee838a0972bd3e50d9e0faa0ba5a
                  - delay:
                      hours: 0
                      minutes: 0
                      seconds: 3
                      milliseconds: 0
                  - action: script.emhass_charging_power_2500
                    data: {}
                else:
                  - action: huawei_solar.forcible_charge_soc
                    data:
                      target_soc: >-
                        {% set value = states('predbat.best_charge_limit') |
                        int(0) %} {{ value if value >= 12 else 12 }}
                      power: 5000
                      device_id: 6893ee838a0972bd3e50d9e0faa0ba5a
                  - delay:
                      hours: 0
                      minutes: 0
                      seconds: 3
                      milliseconds: 0
                  - action: script.huawei_charging_power_5000w
                    data: {}
              - delay:
                  hours: 0
                  minutes: 0
                  seconds: 3
                  milliseconds: 0
              - action: script.huawei_discharging_power_0w
                data: {}
            enabled: true
        alias: Charge
      - conditions:
          - condition: and
            conditions:
              - condition: state
                entity_id: binary_sensor.predbat_charging
                state: "off"
              - condition: state
                entity_id: binary_sensor.predbat_exporting
                state: "on"
        sequence:
          - alias: Discharge
            if:
              - condition: numeric_state
                entity_id: predbat.best_export_limit
                below: 12
              - condition: state
                entity_id: predbat.status
                state: Exporting
            then:
              - data:
                  duration: 120
                  device_id: 6893ee838a0972bd3e50d9e0faa0ba5a
                  power: "5000"
                action: huawei_solar.forcible_discharge
              - delay:
                  hours: 0
                  minutes: 0
                  seconds: 5
                  milliseconds: 0
              - action: script.huawei_discharging_power_5000
                data: {}
            else:
              - action: huawei_solar.forcible_discharge_soc
                data:
                  target_soc: >-
                    {% set value = states('predbat.best_export_limit') | int(0)
                    %} {{ value if value >= 12 else 12 }}
                  power: "5000"
                  device_id: 6893ee838a0972bd3e50d9e0faa0ba5a
              - delay:
                  hours: 0
                  minutes: 0
                  seconds: 5
                  milliseconds: 0
              - action: script.huawei_discharging_power_5000
                data: {}
        alias: Discharge
  - alias: "STOP Freeze Hold Demand "
    choose:
      - conditions:
          - condition: state
            entity_id: predbat.status
            state: Hold for Car
        sequence:
          - action: script.huawei_discharging_power_0w
            data: {}
      - conditions:
          - condition: or
            conditions:
              - condition: state
                entity_id: predbat.status
                state: Hold exporting
              - condition: state
                entity_id: predbat.status
                state: Freeze exporting
        sequence:
          - action: script.huawei_solar_stop_fcfdc_power_0w_export_5000w_import
            metadata: {}
            data: {}
        alias: If Status is Freeze exporting or Hold exporting
      - conditions:
          - condition: or
            conditions:
              - condition: state
                entity_id: predbat.status
                state: Hold charging
              - condition: state
                entity_id: predbat.status
                state: Freeze charging
        sequence:
          - action: script.huawei_solar_stop_fcfdc_power_5000w_export_0w_import
            metadata: {}
            data: {}
        alias: If Status is Hold charging, Freeze Charging
      - conditions:
          - condition: state
            entity_id: predbat.status
            state: Demand
        sequence:
          - action: script.huawei_solar_stop_fcfdc_power_5000w
            metadata: {}
            data: {}
  - alias: Car charging slot On/Off
    choose:
      - conditions:
          - condition: trigger
            id:
              - Predbat car charging slot
          - condition: or
            conditions:
              - condition: state
                entity_id: predbat.status
                state: Hold charging
              - condition: state
                entity_id: predbat.status
                state: Freeze charging
        sequence:
          - if:
              - condition: state
                entity_id: binary_sensor.predbat_car_charging_slot
                state: "off"
            then:
              - action: button.press
                metadata: {}
                data: {}
                target:
                  entity_id: button.go_echarger_265216_frc_2
            alias: If Predbat car charging slot is Off
          - if:
              - condition: state
                entity_id: binary_sensor.predbat_car_charging_slot
                state: "on"
            then:
              - action: button.press
                metadata: {}
                data: {}
                target:
                  entity_id: button.go_echarger_265216_frc_3
              - action: script.huawei_discharging_power_0w
                metadata: {}
                data: {}
              - if:
                  - condition: state
                    entity_id: predbat.status
                    state: Charging
                then:
                  - action: script.emhass_charging_power_2500
                    data: {}
            enabled: true
            alias: If Predbat car charging slot is On
        alias: Car Charging Slot
mode: parallel
max: 10
